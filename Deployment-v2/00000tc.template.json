{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "name": {
            "type": "String"
        },
        "location": {
            "type": "String"
        },
        "locationkey": {
            "type": "String"
        },
        "account": {
            "type": "String"
        },
        "accountsubscription": {
            "type": "String"
        },
        "accountgroup": {
            "type": "String"
        }
    },
    "resources": [
        {
            "type": "Microsoft.Web/connections",
            "apiVersion": "2016-06-01",
            "name": "[parameters('name')]",
            "location": "[parameters('location')]",
            "kind": "V1",
            "properties": {
                "displayName": "[parameters('name')]",
                "customParameterValues": {},
                "statuses": [
                    {
                        "status": "Connected"
                    }
                ],
                "parameterValues": {
                    "databaseAccount": "[parameters('account')]",
                     "accessKey": "[listKeys(concat('/subscriptions/',parameters('accountsubscription'),'/resourceGroups/',parameters('accountgroup'),'/providers/Microsoft.DocumentDB/databaseAccounts/',parameters('account')), '2015-04-08').primaryMasterKey]"
                },
                "api": {
                    "id": "[concat('/subscriptions/',parameters('accountsubscription'),'/providers/Microsoft.Web/locations/',parameters('locationkey'),'/managedApis/documentdb')]"
                }
            }
        }
    ]
}
