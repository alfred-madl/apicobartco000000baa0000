{% assign topbody = topbody | plus:25 %}
{% endif %}	

{% if DeliveryReceipt %}
						<div id="dran" class="drw">
{% if iconinfo == 0 %}
										<div class="iconinfo"> 
{% else %}
										<div class="iconinfo" style="display: inline;">
{% endif %}
											<div class="drnamew">
													<div class="drinfo">
														This message was sent with Delivery receipt requested.
													</div>
											</div>
										</div>
									</div>	
{% assign iconinfo = 1 %}
{% assign topbody = topbody | plus:25 %}
{% endif %}		

{% if signature.sok == true %}
						<div id="dran" class="drw">
										<div class="iconsignok"> 
											<div class="drnamew">
													<div class="drinfo">
														Signed by {{ signature.sub | replace:'E=','' }}
													</div>
											</div>
										</div>
									</div>
{% assign topbody = topbody | plus:25 %}
{% endif %}	

{% if signature.sok == false %}
						<div id="dran" class="drw">
										<div class="iconsignok_f"> 
											<div class="drnamew">
													<div class="drinfo">
														There are problems with the signature.
													</div>
											</div>
										</div>
									</div>
{% assign topbody = topbody | plus:25 %}
{% endif %}	

{% if Attachments_Base64.size > 0 %}		
							<div class="newmailattachedframe" id="newmailattachedframe">
								<div id="fatframew" onscroll="hideallselectmenus()">
									<div class="filearraymsg">
									<ul class="msgattachment" style=" background-color: #E0E0E0; height: 19px; padding: 2px 0px 0px 0px;">
										<div class="text" style="padding-top: 4px;">Message</div>
										</ul>									
										<ul class="filearray" >										
										</li>	
{% for item in Attachments_Base64 %}										
											<li class="tf{{item[3]}}">													  
													<a class="tooltip_common">
														<div class="atname">
															<div class="atnamew" id="atnamew {{item[0]}}">
																<div class="atnames" id="atnames {{item[0]}}"  style="font-size: 11px;">
																	{{item[0]}}</div>
																<div style="font-size: 11px;">
																	&nbsp;({{item[2]}} B)
															</div>															
															</div>															
														</div>														
													</a>												
											</li>
{% endfor %}                                           
										</ul>
									</div>
								</div>
							</div>	
{% else %}
{% assign topbody = topbody | minus:40 %}
{% endif %}
							<div class="mailcontentframe" id="mailcontentframe" style="top: {{topbody}}px">
								<div class="macontholder" id="contentHolder_{{Subject}}">
{% if BodyHTML.size > 0 %}
{{ BodyHTML }}
<BR>
{% else %}
{{ BodyText | newline_to_br }}
<BR>
{% endif %}
								</div>					
	
	</body>
</html>